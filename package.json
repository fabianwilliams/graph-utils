{
  "name": "graph-utils",
  "version": "1.0.0",
  "description": "Utilities for working with graphs in TypeScript for use with MCP",
  "keywords": [
    "msgraph",
    "microsoft",
    "graph",
    "microsoft-graph",
    "microsoft-graph-sdk",
    "typescript"
  ],
  "homepage": "https://github.com/fabianwilliams/graph-utils",
  "bugs": {
    "url": "https://github.com/fabianwilliams/graph-utils/issues"
  },
  "author": "Fabian G Williams <fabian@adotob.com> (https://go.fabswill.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabianwilliams/graph-utils.git"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "ci": "npm run build && npm run check-format && npm run lint && npm run test",
    "test": "vitest run",
    "dev": "vitest"
  }
}
